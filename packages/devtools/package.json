{
  "name": "@next-devtools/core",
  "version": "0.1.0",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/xinyao27/next-devtools",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xinyao27/next-devtools.git",
    "directory": "packages/devtools"
  },
  "author": {
    "name": "Xinyao",
    "email": "hi@xinyao.me"
  },
  "files": [
    "*.cjs",
    "*.d.ts",
    "*.mjs",
    "dist"
  ],
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./plugin": {
      "types": "./dist/plugin.d.ts",
      "require": "./dist/plugin.cjs",
      "import": "./dist/plugin.js"
    },
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "dependencies": {
    "@next-devtools/shared": "workspace:*",
    "@next/env": "^14.0.4",
    "@trpc/client": "^10.45.0",
    "@trpc/server": "^10.45.0",
    "consola": "^3.2.3",
    "dotenv": "^16.3.1",
    "execa": "^8.0.1",
    "fast-glob": "^3.3.2",
    "get-port-please": "^3.1.1",
    "react-dev-inspector": "^2.0.0",
    "react-docgen": "^7.0.1",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.46",
    "@types/ws": "^8.5.10",
    "next": "^14.0.4",
    "webpack": "^5.89.0"
  }
}
